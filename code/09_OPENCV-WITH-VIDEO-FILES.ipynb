    "\n",
    "if cap.isOpened() == False:\n",
    "    print('ERROR FILE NOT FOUND or WRONG!')\n",
    "    \n",
    "while cap.isOpened():\n",
    "    \n",
    "    ret,frame = cap.read()\n",
    "    \n",
    "    if ret == True>\n",
    "    \n",
    "        cv2.imshow('frame',frame)\n",
